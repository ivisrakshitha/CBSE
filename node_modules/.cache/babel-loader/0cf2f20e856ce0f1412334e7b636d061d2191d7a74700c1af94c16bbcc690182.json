w-x: auto;\r\n  overflow-y: visible;\r\n}\r\n\r\n.class-card {\r\n  position: relative;\r\n  width: 320px;\r\n  height: 400px;\r\n  background: linear-gradient(135deg, #2a8888 0%, #1a6666 100%);\r\n  border-radius: 20px;\r\n  box-shadow: \r\n    0 20px 40px rgba(0, 0, 0, 0.3),\r\n    0 10px 20px rgba(23, 181, 181, 0.1),\r\n    inset 0 1px 0 rgba(255, 255, 255, 0.1);\r\n  cursor: pointer;\r\n  transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);\r\n  transform-style: preserve-3d;\r\n  flex-shrink: 0;\r\n  \r\n  /* Clean left-to-right 3D positioning like image 1 */\r\n  transform: \r\n    translateX(calc(var(--index) * -60px))\r\n    rotateY(-15deg)\r\n    rotateX(5deg);\r\n  \r\n  margin-left: calc(var(--index) * 50px);\r\n}\r\n\r\n.class-card:hover {\r\n  transform: \r\n    translateX(calc(var(--index) * -60px))\r\n    translateY(-15px)\r\n    rotateY(-15deg)\r\n    rotateX(5deg)\r\n    scale(1.05);\r\n  box-shadow: \r\n    0 30px 60px rgba(0, 0, 0, 0.4),\r\n    0 15px 30px rgba(23, 181, 181, 0.2),\r\n    inset 0 1px 0 rgba(255, 255, 255, 0.2);\r\n}\r\n\r\n.class-content {\r\n  padding: 30px;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  position: relative;\r\n  color: white;\r\n}\r\n\r\n.class-header {\r\n  margin-bottom: 20px;\r\n  z-index: 2;\r\n}\r\n\r\n.class-header h2 {\r\n  font-size: 2.2rem;\r\n  font-weight: 700;\r\n  margin-bottom: 8px;\r\n  color: #ffffff;\r\n  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.class-header p {\r\n  font-size: 0.9rem;\r\n  color: rgba(255, 255, 255, 0.85);\r\n  line-height: 1.4;\r\n  font-weight: 400;\r\n}\r\n\r\n.class-number {\r\n  font-size: 8rem;\r\n  font-weight: 900;\r\n  color: rgba(255, 255, 255, 0.12);\r\n  text-align: center;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  z-index: 0;\r\n  text-shadow: 0 2px 8px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.class-grade {\r\n  position: absolute;\r\n  bottom: 25px;\r\n  right: 30px;\r