{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport IvisLabsLogo from '../components/IvisLabsLogo.vue';\nexport default {\n  name: 'SubjectsPage',\n  components: {\n    IvisLabsLogo\n  },\n  data() {\n    return {\n      subjects: ['Mathematics', 'Science', 'Social Science', 'English', 'Kannada', 'Hindi'],\n      hoverIndex: -1\n    };\n  },\n  computed: {\n    classNum() {\n      return parseInt(this.$route.params.classNum) || 10;\n    },\n    centerIndex() {\n      return (this.subjects.length - 1) / 2;\n    }\n  },\n  methods: {\n    selectSubject(subject) {\n      this.$router.push({\n        name: 'Textbooks',\n        params: {\n          classNum: this.classNum,\n          subject: subject.toLowerCase().replace(' ', '-')\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["IvisLabsLogo","name","components","data","subjects","hoverIndex","computed","classNum","parseInt","$route","params","centerIndex","length","methods","selectSubject","subject","$router","push","toLowerCase","replace"],"sources":["C:\\Users\\ranja\\Desktop\\cbse.ivslabs\\cbse\\cbse\\src\\views\\SubjectsPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"subjects-page\">\r\n    <nav class=\"navbar\">\r\n      <router-link to=\"/\" class=\"nav-logo\">\r\n        <IvisLabsLogo />\r\n      </router-link>\r\n      <div class=\"nav-pulse\">PULSE</div>\r\n      <div class=\"breadcrumbs\">\r\n        <router-link to=\"/cbse\">CBSE</router-link> /\r\n        <span>Class {{ classNum }}</span>\r\n      </div>\r\n    </nav>\r\n\r\n    <header>\r\n      <h1>Class {{ classNum }} Subjects</h1>\r\n      <p>Select a subject to explore curriculum and resources</p>\r\n    </header>\r\n\r\n    <div class=\"subjects-container\">\r\n      <div\r\n        v-for=\"(subject, index) in subjects\"\r\n        :key=\"index\"\r\n        class=\"subject-card\"\r\n        :class=\"{ active: hoverIndex === index }\"\r\n        :style=\"{\r\n          transform: `translateX(${(index 