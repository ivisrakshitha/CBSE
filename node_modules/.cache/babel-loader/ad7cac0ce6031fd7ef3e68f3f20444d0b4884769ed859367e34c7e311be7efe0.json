{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport IvisLabsLogo from '../components/IvisLabsLogo.vue';\nexport default {\n  name: 'CbsePage',\n  components: {\n    IvisLabsLogo\n  },\n  methods: {\n    selectClass(classNum) {\n      if (classNum === 10) {\n        // Navigate to Class 10 subjects page\n        this.$router.push(`/class/${classNum}/subjects`);\n      } else {\n        // For future classes when they become available\n        console.log(`Selected Class ${classNum}`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["IvisLabsLogo","name","components","methods","selectClass","classNum","$router","push","console","log"],"sources":["/home/johnaic-admin/IVIS LABS/CBSE/cbse/src/views/CbsePage.vue"],"sourcesContent":["<!-- src/views/CbsePage.vue -->\n<template>\n  <div class=\"cbse-page\">\n    <nav class=\"navbar\">\n      <router-link to=\"/\" class=\"nav-logo\">\n        <IvisLabsLogo />\n      </router-link>\n      <div class=\"nav-pulse\">PULSE</div>\n    </nav>\n    \n    <header>\n      <h1>CBSE Classes</h1>\n      <p>Select a class to get started</p>\n    </header>\n    \n    <div class=\"class-grid\">\n      <div \n        v-for=\"classNum in 10\" \n        :key=\"classNum\" \n        class=\"class-card\"\n        :class=\"{ 'disabled': classNum < 10 }\"\n        @click=\"classNum === 10 ? selectClass(classNum) : null\"\n      >\n        <div class=\"class-content\">\n          <h2>Class {{ classNum }}</h2>\n          <p>Explore curriculum, resources, and more</p>\n          <span class=\"card-icon\">\n            <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\">\n              <path d=\"M4 19.5A2.5 2.5 0 0 1 6.5 17H20\"></path>\n              <path d=\"M6.5 2H20v20H6.5A2.5 2.5 0 0 1 4 19.5v-15A2.5 2.5 0 0 1 6.5 2z\"></path>\n            </svg>\n          </span>\n          <div v-if=\"classNum < 10\" class=\"coming-soon-tag\">Coming Soon</div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport IvisLabsLogo from '../components/IvisLabsLogo.vue'\n\nexport default {\n  name: 'CbsePage',\n  components: {\n    IvisLabsLogo\n  },\n  methods: {\n    selectClass(classNum) {\n      if (classNum === 10) {\n        // Navigate to Class 10 subjects page\n        this.$router.push(`/class/${classNum}/subjects`);\n      } else {\n        // For future classes when they become available\n        console.log(`Selected Class ${classNum}`);\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.cbse-page {\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 40px 20px;\n  background-color: #121212;\n  min-height: 100vh;\n}\n\n.navbar {\n  display: flex;\n  align-items: center;\n  padding: 20px;\n  margin-bottom: 30px;\n  border-bottom: 1px solid #2c2c2c;\n}\n\n.nav-logo {\n  text-decoration: none;\n}\n\n.nav-pulse {\n  margin-left: 15px;\n  font-size: 1.8rem;\n  font-weight: 600;\n  color: #ffffff;\n}\n\nheader {\n  text-align: center;\n  margin-bottom: 50px;\n}\n\nheader h1 {\n  font-size: 2.5rem;\n  color: #17b5b5;\n  margin-bottom: 10px;\n}\n\nheader p {\n  font-size: 1.2rem;\n  color: #e0e0e0;\n}\n\n.class-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 25px;\n}\n\n.class-card {\n  background: #1e1e1e;\n  border-radius: 12px;\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);\n  transition: all 0.3s ease;\n  cursor: pointer;\n  overflow: hidden;\n  border: 1px solid #2c2c2c;\n}\n\n.class-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 10px 20px rgba(23, 181, 181, 0.15);\n  border-color: #17b5b5;\n}\n\n.class-content {\n  padding: 25px;\n  position: relative;\n}\n\n.class-card h2 {\n  font-size: 1.5rem;\n  color: #17b5b5;\n  margin-bottom: 10px;\n}\n\n.class-card p {\n  color: #b0b0b0;\n  font-size: 0.9rem;\n  margin-bottom: 15px;\n}\n\n.card-icon {\n  display: inline-block;\n  color: #17b5b5;\n  margin-top: 10px;\n}\n\n@media (max-width: 768px) {\n  .class-grid {\n    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\n  }\n}\n.class-card.disabled {\n  opacity: 0.6;\n  cursor: not-allowed;\n  filter: grayscale(50%);\n}\n\n.class-card.disabled:hover {\n  transform: none;\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);\n  border-color: #2c2c2c;\n}\n\n.coming-soon-tag {\n  position: absolute;\n  top: 10px;\n  right: 10px;\n  background-color: rgba(0, 0, 0, 0.7);\n  color: #17b5b5;\n  padding: 4px 8px;\n  border-radius: 4px;\n  font-size: 0.7rem;\n  font-weight: 600;\n}\n</style>"],"mappings":";AAwCA,OAAOA,YAAW,MAAO,gCAA+B;AAExD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,OAAO,EAAE;IACPC,WAAWA,CAACC,QAAQ,EAAE;MACpB,IAAIA,QAAO,KAAM,EAAE,EAAE;QACnB;QACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,UAAUF,QAAQ,WAAW,CAAC;MAClD,OAAO;QACL;QACAG,OAAO,CAACC,GAAG,CAAC,kBAAkBJ,QAAQ,EAAE,CAAC;MAC3C;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}