{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport IvisLabsLogo from '../components/IvisLabsLogo.vue';\nexport default {\n  name: 'CbsePage',\n  components: {\n    IvisLabsLogo\n  },\n  data() {\n    return {\n      animatedCards: []\n    };\n  },\n  mounted() {\n    this.animateCards();\n  },\n  methods: {\n    selectClass(classNum) {\n      if (classNum === 10) {\n        this.$router.push(`/class/${classNum}/subjects`);\n      }\n    },\n    animateCards() {\n      for (let i = 0; i < 10; i++) {\n        setTimeout(() => {\n          this.animatedCards.push(i + 1);\n        }, i * 400);\n      }\n    }\n  }\n};","map":{"version":3,"names":["IvisLabsLogo","name","components","data","animatedCards","mounted","animateCards","methods","selectClass","classNum","$router","push","i","setTimeout"],"sources":["C:\\Users\\ranja\\Desktop\\cbse.ivslabs\\cbse\\cbse\\src\\views\\CbsePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"cbse-page\">\r\n    <nav class=\"navbar\">\r\n      <router-link to=\"/\" class=\"nav-logo\">\r\n        <IvisLabsLogo />\r\n      </router-link>\r\n      <div class=\"nav-pulse\">PULSE</div>\r\n    </nav>\r\n\r\n    <header>\r\n      <h1>CBSE Classes</h1>\r\n      <p>Select a class to get started</p>\r\n    </header>\r\n\r\n    <div class=\"class-container\">\r\n      <div \r\n        v-for=\"classNum in 10\" \r\n        :key=\"classNum\" \r\n        class=\"class-card\"\r\n        :class=\"{\r\n          'disabled': classNum < 10,\r\n          'animated': animatedCards.includes(classNum)\r\n        }\"\r\n        :style=\"{ \r\n          '--index': classNum - 1,\r\n          'z-index': 11 - classNum\r\n        }\"\r\n        @click=\"classNum === 10 ? selectClass(classNum) : null\"\r\n      >\r\n        <div class=\"glass-content\">\r\n          <h2 class=\"glass-title\">Class {{ classNum }}</h2>\r\n          <p class=\"glass-subtext\">Explore curriculum, resources and more</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport IvisLabsLogo from '../components/IvisLabsLogo.vue'\r\n\r\nexport default {\r\n  name: 'CbsePage',\r\n  components: {\r\n    IvisLabsLogo\r\n  },\r\n  data() {\r\n    return {\r\n      animatedCards: []\r\n    }\r\n  },\r\n  mounted() {\r\n    this.animateCards();\r\n  },\r\n  methods: {\r\n    selectClass(classNum) {\r\n      if (classNum === 10) {\r\n        this.$router.push(`/class/${classNum}/subjects`);\r\n      }\r\n    },\r\n    animateCards() {\r\n      for (let i = 0; i < 10; i++) {\r\n        setTimeout(() => {\r\n          this.animatedCards.push(i + 1);\r\n        }, i * 400);\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.cbse-page {\r\n  max-width: 1600px;\r\n  margin: 0 auto;\r\n  padding: 40px 20px;\r\n  background: radial-gradient(ellipse at center, #0d0d0d 0%, #050505 100%);\r\n  min-height: 100vh;\r\n  overflow: hidden;\r\n}\r\n\r\n.navbar {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 10px 15px;\r\n  margin-bottom: 20px;\r\n  border-bottom: 1px solid #2c2c2c;\r\n}\r\n\r\n.nav-logo {\r\n  text-decoration: none;\r\n}\r\n\r\n.nav-pulse {\r\n  margin-left: 15px;\r\n  font-size: 1.4rem;\r\n  font-weight: 600;\r\n  color: #ffffff;\r\n}\r\n\r\nheader {\r\n  text-align: center;\r\n  margin-bottom: 60px;\r\n}\r\n\r\nheader h1 {\r\n  font-size: 3rem;\r\n  color: #17b5b5;\r\n  margin-bottom: 15px;\r\n  font-weight: 700;\r\n  text-shadow: 0 0 15px rgba(23, 181, 181, 0.5);\r\n}\r\n\r\nheader p {\r\n  font-size: 1.2rem;\r\n  color: #cccccc;\r\n  opacity: 0.85;\r\n}\r\n\r\n.class-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  perspective: 1200px;\r\n  min-height: 450px;\r\n  width: 100%;\r\n  position: relative;\r\n}\r\n\r\n.class-card {\r\n  position: absolute;\r\n  width: 240px;\r\n  height: 360px;\r\n  border-radius: 20px;\r\n  background: rgba(255, 255, 255, 0.06);\r\n  backdrop-filter: blur(16px);\r\n  -webkit-backdrop-filter: blur(16px);\r\n  border: 1px solid rgba(255, 255, 255, 0.08);\r\n  box-shadow:\r\n    0 0 30px rgba(0, 255, 255, 0.04),\r\n    0 0 20px rgba(0, 255, 255, 0.06),\r\n    0 0 15px rgba(0, 255, 255, 0.05);\r\n  cursor: pointer;\r\n  overflow: hidden;\r\n  transform-style: preserve-3d;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(calc(-50% + var(--index) * 80px), -50%) rotateY(-35deg) translateZ(calc(var(--index) * -20px));\r\n  transition: transform 0.4s ease, box-shadow 0.4s ease;\r\n}\r\n\r\n.class-card:hover {\r\n  transform: translate(calc(-50% + var(--index) * 80px), -50%) rotateY(-25deg) scale(1.04);\r\n  box-shadow:\r\n    0 0 40px rgba(0, 255, 255, 0.3),\r\n    0 0 15px rgba(0, 255, 255, 0.15);\r\n  border-color: rgba(23, 181, 181, 0.4);\r\n}\r\n\r\n.glass-content {\r\n  padding: 30px 20px;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: flex-end;\r\n  text-align: center;\r\n}\r\n\r\n.glass-title {\r\n  font-size: 1.8rem;\r\n  font-weight: 700;\r\n  color: #ffffff;\r\n  margin-bottom: 10px;\r\n  text-shadow: 0 0 10px rgba(23, 181, 181, 0.4);\r\n}\r\n\r\n.glass-subtext {\r\n  font-size: 0.9rem;\r\n  color: #e0ffff;\r\n  opacity: 0.75;\r\n}\r\n\r\n.class-card.disabled {\r\n  opacity: 0.4;\r\n  pointer-events: none;\r\n}\r\n</style>\r\n"],"mappings":";AAuCA,OAAOA,YAAW,MAAO,gCAA+B;AAExD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE;IACjB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAACC,QAAQ,EAAE;MACpB,IAAIA,QAAO,KAAM,EAAE,EAAE;QACnB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,UAAUF,QAAQ,WAAW,CAAC;MAClD;IACF,CAAC;IACDH,YAAYA,CAAA,EAAG;MACb,KAAK,IAAIM,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,EAAE,EAAEA,CAAC,EAAE,EAAE;QAC3BC,UAAU,CAAC,MAAM;UACf,IAAI,CAACT,aAAa,CAACO,IAAI,CAACC,CAAA,GAAI,CAAC,CAAC;QAChC,CAAC,EAAEA,CAAA,GAAI,GAAG,CAAC;MACb;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module