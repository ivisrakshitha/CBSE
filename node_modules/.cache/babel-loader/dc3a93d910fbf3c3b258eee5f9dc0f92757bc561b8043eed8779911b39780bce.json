useleave=\"hoverIndex = -1\"\r\n        @click=\"selectSubject(subject)\"\r\n      >\r\n        <div class=\"card-header\">\r\n          <div class=\"subject-header\">\r\n            <h2>{{ subject }}</h2>\r\n            <p>Explore curriculum, resources and more</p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"card-body\">\r\n          <div class=\"subject-icon\" :class=\"'icon-' + subject.toLowerCase().replace(' ', '-')\">\r\n            <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"32\" height=\"32\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\">\r\n              <path d=\"M2 3h6a4 4 0 0 1 4 4v14a3 3 0 0 0-3-3H2z\"></path>\r\n              <path d=\"M22 3h-6a4 4 0 0 0-4 4v14a3 3 0 0 1 3-3h7z\"></path>\r\n            </svg>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport IvisLabsLogo from '../components/IvisLabsLogo.vue'\r\n\r\nexport default {\r\n  name: 'SubjectsPage',\r\n  components: {\r\n    IvisLabsLogo\r\n  },\r\n  data() {\r\n    return {\r\n      subjects: ['Mathematics', 'Science', 'Social Science', 'English', 'Kannada', 'Hindi'],\r\n      hoverIndex: -1,\r\n      animatedCards: []\r\n    }\r\n  },\r\n  mounted() {\r\n    this.animateCards();\r\n  },\r\n  computed: {\r\n    classNum() {\r\n      return parseInt(this.$route.params.classNum) || 10;\r\n    },\r\n    centerIndex() {\r\n      return (this.subjects.length - 1) / 2;\r\n    }\r\n  },\r\n  methods: {\r\n    selectSubject(subject) {\r\n      this.$router.push({\r\n        name: 'Textbooks',\r\n        params: {\r\n          classNum: this.classNum,\r\n          subject: subject.toLowerCase().replace(' ', '-')\r\n        }\r\n      });\r\n    },\r\n    animateCards() {\r\n      this.subjects.forEach((_, index) => {\r\n        setTimeout(() => {\r\n          this.animatedCards.push(index);\r\n        }, index * 150);\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.subjects-page {\r\n  max-width: 1600px;\r\n  margin: 0 auto;\r\n  padding: 40px 20px;\r\n  background: linear-gradient(135deg, #0a0a0a 0%, #1a1a1a 100%);\r\n  min-height: 100vh;\r\n  overflow: hidden;\r\n}\r\n\r\n.navbar {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 10px 15px;\r\n  margin-bottom: 20px;\r\n  border-bottom: 1px solid #2c2c2c;\r\n}\r\n\r\n.nav-logo {\r\n  text-decoration: none;\r\n}\r\n\r\n.nav-pulse {\r\n  margin-left: 15px;\r\n  font-size: 1.4rem;\r\n  font-weight: 600;\r\n  color: #ffffff;\r\n}\r\n\r\n.breadcrumbs {\r\n  margin-left: auto;\r\n  font-size: 1rem;\r\n  color: #b0b0b0;\r\n}\r\n\r\n.breadcrumbs a {\r\n  color: #17b5b5;\r\n  text-decoration: none;\r\n}\r\n\r\n.breadcrumbs a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\nheader {\r\n  text-align: center;\r\n  margin-bottom: 60px;\r\n}\r\n\r\nheader h1 {\r\n  font-size: 3rem;\r\n  color: #17b5b5;\r\n  margin-bottom: 15px;\r\n  font-weight: 700;\r\n  text-shadow: 0 0 15px rgba(23, 181, 181, 0.4);\r\n}\r\n\r\nheader p {\r\n  font-size: 1.3rem;\r\n  color: #e0e0e0;\r\n  opacity: 0.8;\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  line-height: 1.6;\r\n}\r\n\r\n.subjects-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  perspective: 1200px;\r\n  min-height: 500px;\r\n  width: 100%;\r\n  position: relative;\r\n  margin-top: 30px;\r\n}\r\n\r\n.subject-card {\r\n  position: absolute;\r\n  width: 300px;\r\n  height: 400px;\r\n  cursor: pointer;\r\n  transition: all 1.4s cubic-bezier(0.25, 0.8, 0.25, 1);\r\n  transform-origin: center center;\r\n  transform: rotateY(-15deg) translateZ(0px) translateX(calc(100vw + 50px));\r\n  opacity: 0;\r\n  top: 50%;\r\n  margin-top: -200px;\r\n\r\n  /* ðŸ’  Glass look */\r\n  background: rgba(255, 255, 255, 0.05);\r\n  border-radius: 20px;\r\n  border: 1px solid rgba(255, 255, 255, 0.15);\r\n  backdrop-filter: blur(18px);\r\n  -webkit-backdrop-filter: blur(18px);\r\n  box-shadow: 0 15px 40px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.subject-card.animated {\r\n  transform: rotateY(-15deg) translateZ(0px) translateX(-50%);\r\n  opacity: 1;\r\n  transition-delay: var(--delay);\r\n}\r\n\r\n.subject-card.active {\r\n  transform: rotateY(0deg) translateZ(50px) scale(1.05) translateX(-50%);\r\n  opacity: 1;\r\n  z-index: 10;\r\n}\r\n\r\n/* Glass Card Header */\r\n.card-header {\r\n  background: #29c1c1;\r\n  padding: 25px 30px 20px 30px;\r\n  position: relative;\r\n  border-radius: 17px 17px 0 0;\r\n  height: 120px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  border-bottom: 1px solid rgba(255, 255, 255, 0.15);\r\n}\r\n\r\n/* Glass Card Body */\r\n.card-body {\r\n  background: rgba(20, 20, 20, 0.3);\r\n  backdrop-filter: blur(12px);\r\n  -webkit-backdrop-filter: blur(12px);\r\n  padding: 30px;\r\n  height: calc(100% - 120px);\r\n  border-radius: 0 0 17px 17px;\r\n  border-top: 1px solid rgba(255, 255, 255, 0.05);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.subject-header h2 {\r\n  font-size: 2.2rem;\r\n  font-weight: 700;\r\n  margin-bottom: 8px;\r\n  color: #ffffff;\r\n  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n\r\n.subject-header p {\r\n  font-size: 0.9rem;\r\n  color: rgba(255, 255, 255, 0.85);\r\n  line-height: 1.4;\r\n  font-weight: 400;\r\n}\r\n\r\n/* Icon inside glass */\r\n.subject-icon {\r\n  width: 120px;\r\n  height: 120px;\r\n  background-color: rgba(23, 181, 181, 0.08);\r\n  border-radius: 50%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  color: #17b5b5;\r\n  border: 2px solid rgba(77, 210, 210, 0.4);\r\n  backdrop-filter: blur(4px);\r\n}\r\n\r\n.subject-card.active .subject-icon {\r\n  transform: scale(1.1);\r\n  background-color: rgba(23, 181, 181, 0.15);\r\n  border-color: #4dd2d2;\r\n  box-shadow: 0 0 20px rgba(23, 181, 181, 0.3);\r\n}\r\n\r\n.subject-icon svg {\r\n  width: 48px;\r\n  height: 48px;\r\n}\r\n\r\n.icon-mathematics svg,\r\n.icon-science svg,\r\n.icon-social-science svg,\r\n.icon-english svg,\r\n.icon-kannada svg,\r\n.icon-hindi svg {\r\n  stroke: #17b5b5;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAe;;EACnBA,KAAK,EAAC;AAAQ;;EAKZA,KAAK,EAAC;AAAa;;EAWrBA,KAAK,EAAC;AAAoB;mBAlBnC;;EAoCaA,KAAK,EAAC;AAAa;;EACjBA,KAAK,EAAC;AAAgB;;EAMxBA,KAAK,EAAC;AAAW;;;;uBA1C5BC,mBAAA,CAoDM,OApDNC,UAoDM,GAnDJC,mBAAA,CASM,OATNC,UASM,GARJC,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC,GAAG;IAACP,KAAK,EAAC;;IAHhCQ,OAAA,EAAAC,QAAA,CAIQ,MAAgB,CAAhBJ,YAAA,CAAgBK,uBAAA,E;IAJxBC,CAAA;gCAMMR,mBAAA,CAAkC;IAA7BH,KAAK,EAAC;EAAW,GAAC,OAAK,sBAC5BG,mBAAA,CAGM,OAHNS,UAGM,GAFJP,YAAA,CAA0CC,sBAAA;IAA7BC,EAAE,EAAC;EAAO;IAR/BC,OAAA,EAAAC,QAAA,CAQgC,MAAII,MAAA,QAAAA,MAAA,OARpCC,gBAAA,CAQgC,MAAI,E;IARpCH,CAAA;gCAAAG,gBAAA,CAQkD,KAC1C,IAAAX,mBAAA,CAAiC,cAA3B,QAAM,GAAAY,gBAAA,CAAGC,QAAA,CAAAC,QAAQ,iB,KAI3Bd,mBAAA,CAGS,iBAFPA,mBAAA,CAAsC,YAAlC,QAAM,GAAAY,gBAAA,CAAGC,QAAA,CAAAC,QAAQ,IAAG,WAAS,iB,0BACjCd,mBAAA,CAA2D,WAAxD,sDAAoD,qB,GAGzDA,mBAAA,CAkCM,OAlCNe,UAkCM,I,kBAjCJjB,mBAAA,CAgCMkB,SAAA,QAnDZC,WAAA,CAoBmCC,KAAA,CAAAC,QAAQ,EApB3C,CAoBgBC,OAAO,EAAEC,KAAK;yBADxBvB,mBAAA,CAgCM;MA9BHwB,GAAG,EAAED,KAAK;MACXxB,KAAK,EAtBb0B,eAAA,EAsBc,cAAc;kBACYL,KAAA,CAAAM,UAAU,KAAKH,KAAK;oBAAyBH,KAAA,CAAAO,aAAa,CAACC,QAAQ,CAACL,KAAK;;MAIxGM,KAAK,EA3BdC,eAAA;mBA2ByCP,KAAK;mBAAwBA,KAAK;+BAAiDA,KAAK,GAAGR,QAAA,CAAAgB,WAAW;;MAKtIC,YAAU,EAAAC,MAAA,IAAEb,KAAA,CAAAM,UAAU,GAAGH,KAAK;MAC9BW,YAAU,EAAAtB,MAAA,QAAAA,MAAA,MAAAqB,MAAA,IAAEb,KAAA,CAAAM,UAAU;MACtBS,OAAK,EAAAF,MAAA,IAAElB,QAAA,CAAAqB,aAAa,CAACd,OAAO;QAE7BpB,mBAAA,CAKM,OALNmC,UAKM,GAJJnC,mBAAA,CAGM,OAHNoC,UAGM,GAFJpC,mBAAA,CAAsB,YAAAY,gBAAA,CAAfQ,OAAO,kB,0BACdpB,mBAAA,CAA6C,WAA1C,wCAAsC,qB,KAI7CA,mBAAA,CAOM,OAPNqC,UAOM,GANJrC,mBAAA,CAKM;MALDH,KAAK,EA5CpB0B,eAAA,EA4CqB,cAAc,YAAmBH,OAAO,CAACkB,WAAW,GAAGC,OAAO;uCACvEvC,mBAAA,CAGM;MAHDwC,KAAK,EAAC,